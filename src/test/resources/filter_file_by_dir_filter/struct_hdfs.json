{
  "vertices": [
    {
      "label": "person",
      "input": {
        "type": "hdfs",
        "path": "${store_path}",
        "core_site_path": "src/test/resources/hdfs_with_core_site_path/core-site.xml",
        "file_filter": {
          "extensions": ["csv"]
        },
        "dir_filter": {
          "include_regex": "^level.*",
          "exclude_regex": "^level2.*"
        },
        "format": "CSV",
        "charset": "UTF-8"
      },
      "field_mapping": {
        "name": "name",
        "age": "age",
        "city": "city"
      }
    }
  ]
}
